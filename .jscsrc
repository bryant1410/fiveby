{
    "fileExtensions": [ ".js" ],
    "requireSpaceBeforeBlockStatements": true,
    "validateQuoteMarks": "'",
    "requireSpaceAfterKeywords": [
      "do",
      "for",
      "if",
      "else",
      "switch",
      "case",
      "try",
      "catch",
      "void",
      "while",
      "with",
      "return",
      "typeof",
      "function"
    ]
}
